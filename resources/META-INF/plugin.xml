<idea-plugin>
    <id>com.chenbang.intellij.plugin.generator.id</id>
    <name>CB Code Generator</name>
    <version>1.0</version>
    <vendor>ChenBang</vendor>

    <description>
        <![CDATA[
    宸邦项目脚手架代码生成器：
    <ul>
        <li>1、生成项目目录结构</li>
        <li>2、基于数据库表结构生成DAO层代码</li>
   </ul>
    ]]>
    </description>

    <change-notes>1.0 发布</change-notes>

    <idea-version since-build="143"/>

    <depends optional="true">com.intellij.database</depends>

    <actions>
        <action id="mabatis-generate-action" class="com.chenbang.intellij.plugin.action.MainAction" text="宸邦生成DAO代码"
                description="宸邦代码生成器">
            <add-to-group group-id="DatabaseViewPopupMenu" anchor="first"/>
        </action>

    </actions>

</idea-plugin>